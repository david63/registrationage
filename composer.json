{
	"name": "david63/registrationage",
	"type": "phpbb-extension",
	"description": "Adds an option to enter a birthdate upon registration and to check that the date is equal to, or greater, than a minimum age.",
	"version": "2.1.0-RC2",
	"time": "2017-01-11",
    "keywords": [
        "phpbb",
        "extension",
        "age",
		"birthdate",
		"registration"
    ],
	"license": "GPL-2.0-only",
	"authors": [
		{
			"name": "David Wood",
			"role": "Developer"
		}
	],
	"require": {
        "php": ">=5.4.0",
        "composer/installers": "~1.0"
    },
	"extra": {
		"display-name": "[*] Registration age",
		"soft-require": {
			"phpbb/phpbb": ">=3.2.0"
		},
		"version-check": {
        	"host": "version.phpbbdev.space",
        	"directory": "/3.2",
        	"filename": "/registration_age_version_file.json"
    	}
	}
}
